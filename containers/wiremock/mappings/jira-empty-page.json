{
  "request": {
    "method": "POST",
    "urlPath": "/rest/api/3/search/jql",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.jql",
          "equalTo": "project = Unknown"
        }
      },
      {
        "matchesJsonPath": "$.fields"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "jira-empty.json"
  }
}