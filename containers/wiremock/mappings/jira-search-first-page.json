{
  "request": {
    "method": "POST",
    "urlPath": "/rest/api/3/search/jql",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.jql",
          "equalTo": "project = AB"
        }
      },
      {
        "matchesJsonPath": "$.fields"
      }
    ],
    "queryParameters": {
      "nextPageToken": {
        "absent": true
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "jira1.json"
  }
}